import java.util.Scanner;

public class b1011 {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        double r = scan.nextDouble();
        double volume = (4.0/3) * 3.14159 * Math.pow(r, 3);
        System.out.printf("VOLUME = %.3f\n", volume);
    }
}
