import java.util.Scanner;

public class b1014 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        float y = sc.nextFloat();
        float combustivelGasto = x / y;
        System.out.printf("%.3f km/l", combustivelGasto);

    }
}
